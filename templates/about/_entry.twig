{% extends "_layouts/main" %}
{% block main %}
  <div class="pageHeader aboutDylan">
    <div>
      <h2 class="textShadow">
        {% if entry.mainHeader is defined %}
          {{ entry.mainHeader }}
        {% endif %}
      </h2>
      <h4>{{ entry.postContent }}</h4>
    </div>
    <div>
      <img src="{{ entry.singleImage.one().url }}" width="580" class="radiusTwentyFive" />
    </div>
  </div>
{% endblock %}