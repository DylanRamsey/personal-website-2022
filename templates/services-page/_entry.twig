{% extends "_layouts/main" %}
{% block main %}
  <h2>What I can do for you...</h2>
  <div class="serviceCards">
    {% for block in entry.serviceCards.all() %}
      <div class="serviceCard">
        <h5>
          {{ block.serviceHeader }}
        </h5>
        <p>
          {{ block.serviceLink }}
        </p>
      </div>
    {% endfor %}
  </div>
{% endblock %}